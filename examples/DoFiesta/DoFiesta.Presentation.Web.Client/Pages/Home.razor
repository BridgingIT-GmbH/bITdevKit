@page "/"
@using YourNamespace
@inject IStringLocalizer<Global> localizer

<PageTitle>DoFiesta</PageTitle>

<MudContainer MaxWidth="MaxWidth.Large" Class="mt-4">
    <MudCarousel TData="object" Style="height:400px;" ShowArrows="true" ShowBullets="false" EnableSwipeGesture="true" AutoCycle="true">
        <MudCarouselItem Transition="Transition.Slide">
            <div class="d-flex flex-column align-center justify-center mud-height-full">
                <Logo />
                <MudPaper Class="pa-8 mud-width-full" Style="background-color: var(--mud-palette-primary);">
                    <MudText Typo="Typo.h3" Align="Align.Center" Color="Color.Surface">Welcome to DoFiesta</MudText>
                    <MudText Typo="Typo.h5" Align="Align.Center" Color="Color.Surface" Class="mt-4">Your Task Management Solution</MudText>
                    <MudButton Variant="Variant.Filled" Color="Color.Secondary" Size="Size.Large" Href="/todos">Get Started</MudButton>
                </MudPaper>
            </div>
        </MudCarouselItem>

        <MudCarouselItem Transition="Transition.Slide">
            <div class="d-flex flex-column align-center justify-center mud-height-full">
                <Logo />
                <MudPaper Class="pa-8 mud-width-full" Style="background-color: var(--mud-palette-secondary);">
                    <MudText Typo="Typo.h3" Align="Align.Center" Color="Color.Surface">Stay Organized</MudText>
                    <MudText Typo="Typo.h5" Align="Align.Center" Color="Color.Surface" Class="mt-4">
                        Manage tasks, set priorities, and track progress
                    </MudText>
                    <div class="d-flex justify-center gap-4 mt-8">
                        <MudIcon Icon="@Icons.Material.Filled.CheckCircle" Color="Color.Surface" Size="Size.Large"/>
                        <MudIcon Icon="@Icons.Material.Filled.Schedule" Color="Color.Surface" Size="Size.Large"/>
                        <MudIcon Icon="@Icons.Material.Filled.Assignment" Color="Color.Surface" Size="Size.Large"/>
                    </div>
                </MudPaper>
            </div>
        </MudCarouselItem>

        <MudCarouselItem Transition="Transition.Slide">
            <div class="d-flex flex-column align-center justify-center mud-height-full">
                <Logo />
                <MudPaper Class="pa-8 mud-width-full" Style="background-color: var(--mud-palette-success);">
                    <MudText Typo="Typo.h3" Align="Align.Center" Color="Color.Surface">Track Progress</MudText>
                    <MudText Typo="Typo.h5" Align="Align.Center" Color="Color.Surface" Class="mt-4">
                        From New to Completed, monitor your task lifecycle
                    </MudText>
                    <div class="d-flex justify-center gap-4 mt-8">
                        <MudProgressCircular Value="100" Color="Color.Surface" Size="Size.Large"/>
                        <MudText Typo="Typo.h2" Color="Color.Surface">100%</MudText>
                    </div>
                </MudPaper>
            </div>
        </MudCarouselItem>
    </MudCarousel>

    <MudDivider Class="my-4" />

    <MudGrid Class="mt-8">
        <MudItem xs="12" sm="4">
            <MudPaper Class="pa-4" Elevation="2">
                <MudText Typo="Typo.h5" Align="Align.Center">Simple</MudText>
                <MudText Align="Align.Center" Class="mt-4">Easy to use interface for managing your daily tasks</MudText>
            </MudPaper>
        </MudItem>
        <MudItem xs="12" sm="4">
            <MudPaper Class="pa-4" Elevation="2">
                <MudText Typo="Typo.h5" Align="Align.Center">Efficient</MudText>
                <MudText Align="Align.Center" Class="mt-4">Quick task creation and status updates</MudText>
            </MudPaper>
        </MudItem>
        <MudItem xs="12" sm="4">
            <MudPaper Class="pa-4" Elevation="2">
                <MudText Typo="Typo.h5" Align="Align.Center">Organized</MudText>
                <MudText Align="Align.Center" Class="mt-4">Keep track of priorities and deadlines</MudText>
            </MudPaper>
        </MudItem>
    </MudGrid>
</MudContainer>

